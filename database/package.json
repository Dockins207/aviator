{
  "name": "aviator-database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "migrate": "node -r dotenv/config ./node_modules/.bin/pg-migrate up",
    "migrate:down": "node -r dotenv/config ./node_modules/.bin/pg-migrate down",
    "migrate:create": "node -r dotenv/config ./node_modules/.bin/pg-migrate create",
    "seed": "node --experimental-specifier-resolution=node scripts/seed.js"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pg-migrate": "^0.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.5"
  }
}
