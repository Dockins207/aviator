{
  "name": "aviator-tests",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "npm run test:frontend && npm run test:backend && npm run test:e2e",
    "test:frontend": "jest --config=frontend/jest.config.js",
    "test:backend": "jest --config=backend/jest.config.js",
    "test:e2e": "cypress run",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "cypress": "^13.6.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  }
}
